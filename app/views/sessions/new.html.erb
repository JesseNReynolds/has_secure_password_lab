<h1>Log in</h1>
<p><%= @error if @error %></p>
<%= form_tag sessions_path, method: :post do %>
    <div>
        <%= label_tag "name" %>
        <%= text_field_tag user[:name]%>
    </div>
    <div>
        <%= label_tag "password" %>
        <%= password_field_tag user[:password] %>
    </div>
    <%= submit_tag "Submit" %>
<%end%>